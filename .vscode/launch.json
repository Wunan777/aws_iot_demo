{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/mock_web.py",
            "args": [
                "--endpoint",
                "${endpoint_url}",
                "--cert",
                "${workspaceFolder}/cert/${thing_name}.pem.crt",
                "--key",
                "${workspaceFolder}/cert/${thing_name}-private.pem.key",
                "--ca_file",
                "${workspaceFolder}/cert/AmazonRootCA1.pem",
                "--thing_name",
                "${thing_name}"
            ],
            "cwd": "${workspaceFolder}",
            "env": {},
            "console": "integratedTerminal",
            "stopOnEntry": false,
            "justMyCode": false
        }
    ]
}

